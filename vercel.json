{
  "version": 2,
  "builds": [
    { "src": "api/*.js", "use": "@vercel/node" },
    { "src": "server.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "api/$1.js" },
    { "src": "/(.*)", "dest": "server.js" }
  ],
  "functions": {
    "api/*.js": {
      "runtime": "@vercel/node@18.x"
    },
    "server.js": {
      "runtime": "@vercel/node@18.x"
    }
  }
}
